position: top;\n  overflow: hidden;\n  object-fit: cover;\n  transition: var(--transition-longer);\n}\n.card-img {\n  background-color: var(--color-periwinkle-dark);\n  overflow: hidden;\n  /* width: 600px; */\n  height: 300px;\n}\n.post-content {\n  margin: 10px 15px 15px 20px;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: flex-start;\n}\n.post-verse {\n  font-size: 18px;\n  margin: 5px 0;\n  align-self: flex-start;\n  color: var(--color-periwinkle);\n    /* position: absolute; */\n}\n.post-title {\n  margin: 5px 0;\n  font-size: 26px;\n  text-decoration: none;\n  color: var(--color-gray-darkest);\n\n}\n.post-snippet {\n  margin: 5px 0 15px;\n  color: var(--color-gray-dark);\n  width: 90%;\n  font-size: 16px;\n}\n.postCard-btn {\n  background-color: var(--color-periwinkle); /* Green */\n  border: none;\n  color: white;\n  padding: 15px 32px;\n  text-align: center;\n  text-decoration: none;\n  font-weight: 700;\n  display: inline-block;\n  font-size: 16px;\n  border-radius: 5px;\n  cursor: pointer;\n  margin: 5px 0 10px;\n}\n.postCard-btn:hover {\n  box-shadow: var(--shadow-bold);\n  background: var(--color-periwinkle-dark);\n}\n\n.postCard:hover {\n  box-shadow: var(--shadow-bold);\n  background: white;\n\n}\n.postCard:hover img {\n  transform: scale(1.1);\n  opacity: 0.65;\n}\n</style>"]}]}